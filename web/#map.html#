<HTML>

<link rel="stylesheet" href="http://cdn.leafletjs.com/leaflet-0.7.3/leaflet.css" />
<link rel="stylesheet" href="mapStyle.css"/>
<script src="http://cdn.leafletjs.com/leaflet-0.7.3/leaflet.js"></script>
<script src="http://code.jquery.com/jquery-latest.min.js"></script>

<div id="map"></div>

<script>
var map = L.map('map').setView([31.7, -98.99], 13);

L.tileLayer('http://{s}.tiles.mapbox.com/v3/djd231.k39lc6co/{z}/{x}/{y}.png', {
             attribution: 'Map data &copy; [...]',
             maxZoom: 18
}).addTo(map);


console.log("Reading JSON Object");
//Get JSON coord objs
$.ajaxSetup(
    { beforeSend: function(xhr) {
       if(xhr.overrideMimeType) {
            console.log("Prep Work");
            xhr.overrideMimeType("application/json");
            }

     }
});

$.getJSON("example_1024.json", function(data) {
   data.coords.forEach(function(d) {
        console.log(d.lat + "," + d.long);
        L.marker([d.lat, d.long]).addTo(map);
    });
});

//var marker = L.marker([36.4057, -100.7924]).addTo(map);
//L.marker([35.488, -101.060]).addTo(map);



</script>

<HTML>
